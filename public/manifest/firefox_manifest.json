{
    "manifest_version": 2,
    "name": "Boilerplate",
    "description": "",
    "version": "0.0.1",
    "author": "",
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "64": "icon.png"
        },
        "default_title": ""
    },
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}